{
    "name": "aulasoftwarelibre/infocurro",
    "description": "Job Board Tutorial with Symfony",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "cocur/slugify": "^3.1",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "hautelook/alice-bundle": "^2.0",
        "knplabs/knp-menu-bundle": "^2.2",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^4.1",
        "symfony/asset": "^3.4",
        "symfony/console": "^3.4",
        "symfony/debug-pack": "^1.0",
        "symfony/expression-language": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^3.4",
        "symfony/security-bundle": "^3.4",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/validator": "^3.4",
        "symfony/web-link": "^3.4",
        "symfony/web-server-bundle": "^3.4",
        "symfony/yaml": "^3.4",
        "white-october/pagerfanta-bundle": "^1.1"
    },
    "require-dev": {
        "behat/mink-browserkit-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "friendsofphp/php-cs-fixer": "^2.10",
        "lakion/mink-debug-extension": "^1.2",
        "sensiolabs/behat-page-object-extension": "^2.1",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Behat\\": "features/bootstrap"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C5RXDEQT6SASBA0W2A5V2HJT",
            "allow-contrib": false
        }
    }
}
